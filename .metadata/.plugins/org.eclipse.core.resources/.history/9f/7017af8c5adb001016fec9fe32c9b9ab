package clases;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		char[] caracteres = String.valueOf(15).toCharArray();
		Arrays.sort(caracteres);
		StrinBuilder texto = new StringBuilder(caracteres.toString())
		String texto = caracteres.toString();
		System.out.println(texto);
		int numFinal = Integer.parseInt(caracteres.toString());
		System.out.println(numFinal);

	}

}
