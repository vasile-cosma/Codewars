import java.util.Scanner;

public class Solucion {
	public static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		int campanyas = 1;

		while (true) {
			int acum = 0;
			campanyas = sc.nextInt();
			if (campanyas == 0)
				break;
			for (int i = 0; i < campanyas; i++)
				;
			int pajaros = sc.nextInt();
			int anillados = sc.nextInt();
			System.out.println(pajaros-anillados);
		}

	}

}
