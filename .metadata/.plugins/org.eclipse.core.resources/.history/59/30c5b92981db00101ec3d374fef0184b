import java.util.Scanner;

public class Cinquecento {
	public static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		int numEntradas = Integer.parseInt(sc.nextLine());
		int entrada;
		int siglo;
		boolean superior = false;;

		for (int i = 0; i < numEntradas; i++) {
			entrada = Integer.parseInt(sc.nextLine());
			StringBuilder sb = new StringBuilder(Integer.toString(entrada));
			int ultimoNum;
			char c = sb.charAt(sb.length() - 1);
			ultimoNum = Integer.parseInt(Character.toString(c));
			
			for (int j = 1; i<sb.length(); i++) {
				if (Integer.parseInt(Character.toString(sb.charAt(j))) < 0){
					superior = true;
				}
			}
			
			switch (sb.length()) {
			case 3:
				
				if (superior) {
					siglo = Integer.parseInt(Character.toString(sb.charAt(0))) + 1;
					System.out.println(siglo);
				} else {
					siglo = Integer.parseInt(Character.toString(sb.charAt(0)));
					System.out.println(siglo);
				}
				superior = false;
				break;
			case 4:
				if (superior) {
					siglo = Integer.parseInt(sb.substring(0, 2)) + 1;
					System.out.println(siglo);
				} else {
					siglo = Integer.parseInt(sb.substring(0, 2));
					System.out.println(siglo);
				}
				superior = false;
				break;
			case 5:
				if (superior) {
					siglo = Integer.parseInt(sb.substring(0, 3)) + 1;
					System.out.println(siglo);
				} else {
					siglo = Integer.parseInt(sb.substring(0, 3));
					System.out.println(siglo);
				}
				break;
			default: 
				System.out.println(1);
				break;
			}
		}
	}

}
