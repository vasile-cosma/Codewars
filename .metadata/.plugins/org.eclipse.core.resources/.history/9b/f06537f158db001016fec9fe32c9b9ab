package clases;

public class DescendingOrder {
	public static int sortDesc(final int num) {
		int numFinal;
		String cadena1;
		String cadena2;
		cadena1 = Integer.toString(num);
		StringBuilder sb = new StringBuilder(cadena1);
		cadena2 = sb.reverse().toString();
		numFinal = Integer.parseInt(cadena2);
		
		return numFinal;
	}
}
