import java.awt.datatransfer.StringSelection;
import java.util.ArrayList;

public class Scramblies {
    
    public static boolean scramble(String str1, String str2) {    
    	char[] arr1 = str1.toCharArray();
    	char[] arr2 = str2.toCharArray();
    	boolean flag = true;
    	for (char c : arr2) {
			if (str1.indexOf(c)==-1) {
				flag = false;
				break;
			}
		}
    	return flag;
    }
}