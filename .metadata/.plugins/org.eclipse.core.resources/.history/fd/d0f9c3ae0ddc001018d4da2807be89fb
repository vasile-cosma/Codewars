import java.util.Scanner;

public class Solucion {
	public static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		int campanyas = 1;

		while (campanyas != 0) {
			int acum = 0;
			campanyas = Integer.parseInt(sc.nextLine());
			for (int i = 0; i < campanyas; i++)
				;
			int pajaros = Integer.parseInt(sc.next());
			int anillados = Integer.parseInt(sc.next());
			System.out.println(pajaros-anillados);
		}

	}

}
