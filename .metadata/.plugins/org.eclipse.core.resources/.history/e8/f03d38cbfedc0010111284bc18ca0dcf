
public class Scramblies {
    
    public static boolean scramble(String str1, String str2) {    
    	char[] arr2 = str2.toCharArray();
    	StringBuilder sb = new StringBuilder(str1);
    	boolean flag = true;
    	int indice;
    	for (char c : arr2) {
    		indice = sb.indexOf((String)c);
			if (indice==-1) {
				flag = false;
				break;
			}
			sb.deleteCharAt(indice);
			
		}
    	return flag;
    }
}