
public class Solution {
	static String toCamelCase(String s){
	    StringBuilder sb = new StringBuilder(s);
	    for (int i = 0; i <s.length(); i++) {
	    	if (sb.charAt(i) == '-' || sb.charAt(i) == '_') {
	    		char c = sb.charAt(i);
	    		sb.setCharAt(i+1, sb.charAt(i+1).toUpperCase());
	    		
	    	}
	    }
		
	  }
}
